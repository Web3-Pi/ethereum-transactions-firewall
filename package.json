{
  "name": "transactionfirewall",
  "version": "1.1.0",
  "description": "A simple POC service to let user accept/reject ehtereum transaction before it reaches RPC endpoint",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig/tsconfig.json && tsc -p test/tsconfig.test.json",
    "start": "node dist/index.js",
    "dev": "ts-node -P tsconfig/tsconfig.json src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "typecheck": "tsc --noEmit -p tsconfig/tsconfig.json && tsc --noEmit -p test/tsconfig.test.json",
    "test:manual": "ts-node -P test/tsconfig.test.json test/txnemitter.ts"
  },
  "author": "jimmysithis",
  "license": "MIT",
  "dependencies": {
    "abi-decoder": "^2.4.0",
    "axios": "^1.6.8",
    "dotenv": "^16.4.5",
    "ethereumjs-util": "^7.1.5",
    "express": "^4.19.2",
    "uuid": "^10.0.0",
    "web3": "^4.11.1",
    "web3-eth-accounts": "^4.3.1",
    "ws": "^8.16.0",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^14.18.63",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  }
}
